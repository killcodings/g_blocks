{
	"name": "cg-blocks",
	"version": "0.1.0",
	"description": "Example block scaffolded with Create Block tool.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"scripts": {
		"build": "wp-scripts build --webpack-src-dir=src/blocks --output-path=dist",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start --webpack-src-dir=src/blocks --output-path=dist",
		"add": "npx @wordpress/create-block --namespace cg --category cg-blocks --variant dynamic --no-plugin",
		"add-static": "npx @wordpress/create-block --namespace cg --category cg-blocks --no-plugin"
	},
	"devDependencies": {
		"@wordpress/scripts": "^26.9.0"
	},
	"dependencies": {
		"@wordpress/api-fetch": "^6.45.0",
		"@wordpress/blocks": "^12.25.0",
		"@wordpress/components": "^25.14.0",
		"@wordpress/data": "^9.18.0",
		"@wordpress/element": "^5.25.0",
		"@wordpress/i18n": "^4.48.0",
		"@wordpress/notices": "^4.17.0",
		"@wordpress/server-side-render": "^4.25.0",
		"classnames": "^2.5.1",
		"react": "^18.2.0",
		"swiper": "^11.0.5"
	}
}
